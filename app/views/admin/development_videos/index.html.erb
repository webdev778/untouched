<header class="header">
  <h1 id="page-title" class="header__heading">Videos of <%= @development.to_s %></h1>
  <p><%= link_to "Back to Development", admin_development_path(@development) %></p>
</header>

<section>
  <%= react_component 'CompositeVideoUploader', props: {
      videoable: DevelopmentSerializer.new(@development).as_json(root: false), 
      type: 'DevelopmentVideo',
      videoable_type: 'Development',
      videos: ActiveModel::ArraySerializer.new(@development.videos, each_serializer: DevelopmentVideoSerializer).as_json(root: false) 
    }
  %>
</section>

